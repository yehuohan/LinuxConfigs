{
    "configurations": {
        "Launch cxx/rust with CodeLLDB": {
            "adapter": "CodeLLDB",
            "configuration": {
                "request": "launch",
                "program": "${workspaceRoot}/main",
                "stopAtEntry": true
            }
        },
        "Launch cxx with cpptools": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "request": "launch",
                "program": "${workspaceRoot}/main",
                "stopAtEntry": true
            }
        },
        "Launch python with debugpy": {
            "adapter": "debugpy",
            "configuration": {
                "request": "launch",
                "type": "python",
                "cwd": "${workspaceRoot}",
                "program": "${workspaceRoot}/test.py",
                "stopOnEntry": true,
                "console": "integratedTerminal"
            }
        }

    }
}
